@model IEnumerable<TableGrid.Models.mTableGrid>


<h2>User Table</h2>
<a href="@Url.Action("Create", "TableGrid")" class="btn btn-primary mb-3">Create User</a>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>id</th>
            <th>fname</th>
            <th>lname</th>
            <th>userName</th>
            <th>email</th>
            <th>age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.id</td>
                <td>@user.fname</td>
                <td>@user.lname</td>
                <td>@user.userName</td>
                <td>@user.email</td>
                <td>@user.age</td>
            </tr>
        }
    </tbody>
</table>